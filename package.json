{
  "name": "odoo-dev-plugin",
  "displayName": "Odoo Dev",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "extensionDependencies": [
    "vscode.git"
  ],
  "contributes": {
    "commands": [
      {
        "command": "odoo-dev-plugin.fetchBranch",
        "title": "Odoo Dev: Fetch"
      },
      {
        "command": "odoo-dev-plugin.createBranch",
        "title": "Odoo Dev: Create"
      },
      {
        "command": "odoo-dev-plugin.deleteBranch",
        "title": "Odoo Dev: Delete"
      },
      {
        "command": "odoo-dev-plugin.checkoutBranch",
        "title": "Odoo Dev: Checkout"
      },
      {
        "command": "odoo-dev-plugin.startServer",
        "title": "Odoo Dev: Start Server"
      },
      {
        "command": "odoo-dev-plugin.debugServer",
        "title": "Odoo Dev: Debug Server"
      },
      {
        "command": "odoo-dev-plugin.startServerWithInstall",
        "title": "Odoo Dev: Start Server With Install..."
      },
      {
        "command": "odoo-dev-plugin.startFreshServer",
        "title": "Odoo Dev: Start Fresh Server"
      },
      {
        "command": "odoo-dev-plugin.debugServerWithInstall",
        "title": "Odoo Dev: Debug Server With Install..."
      },
      {
        "command": "odoo-dev-plugin.startServerWithUpdate",
        "title": "Odoo Dev: Start Server With Update..."
      },
      {
        "command": "odoo-dev-plugin.debugServerWithUpdate",
        "title": "Odoo Dev: Debug Server With Update..."
      },
      {
        "command": "odoo-dev-plugin.startSelectedTest",
        "title": "Odoo Dev: Start Selected Test"
      },
      {
        "command": "odoo-dev-plugin.debugSelectedTest",
        "title": "Odoo Dev: Debug Selected Test"
      },
      {
        "command": "odoo-dev-plugin.startCurrentTestFile",
        "title": "Odoo Dev: Start Current Test File"
      },
      {
        "command": "odoo-dev-plugin.debugCurrentTestFile",
        "title": "Odoo Dev: Debug Current Test File"
      },
      {
        "command": "odoo-dev-plugin.dropActiveDB",
        "title": "Odoo Dev: Drop Active DB"
      },
      {
        "command": "odoo-dev-plugin.getTestTag",
        "title": "Odoo Dev: Get Test Tag"
      },
      {
        "command": "odoo-dev-plugin.getLocalServerUrl",
        "title": "Odoo Dev: Get Server Url"
      },
      {
        "command": "odoo-dev-plugin.openOdooConf",
        "title": "Odoo Dev: Open Config File"
      }
    ],
    "configuration": {
      "title": "Odoo Dev",
      "properties": {
        "odooDev.sourceFolder": {
          "type": "string",
          "description": "Folder where odoo/odoo repository is clone. This folder is also expected to have odoo/enterprise, odoo/upgrade and your custom addons folder."
        },
        "odooDev.baseBranches": {
          "type": "object",
          "additionalProperties": {
            "type": "number"
          },
          "default": {
            "master": 0
          },
          "description": "List of base branches. Value serves as the sequence number for ordering the items in the Tree View."
        },
        "odooDev.remoteOdooDevUrl": {
          "type": "string",
          "default": "git@github.com:odoo-dev/odoo.git",
          "description": "This is where odoo dev branches will be fetched."
        },
        "odooDev.remoteEnterpriseDevUrl": {
          "type": "string",
          "default": "git@github.com:odoo-dev/enterprise.git",
          "description": "This is where enterprise dev branches will be fetched."
        },
        "odooDev.remoteUpgradeUrl": {
          "type": "string",
          "default": "git@github.com:odoo/upgrade.git",
          "description": "This is where upgrade dev branches will be fetched."
        },
        "odooDev.branchNameAsDB": {
          "title": "Branch Name as DB",
          "type": "boolean",
          "default": false,
          "description": "Use the name of the branch as the name of the db."
        },
        "odooDev.odooConfigPath": {
          "title": "Config Path",
          "type": "string",
          "default": "",
          "description": "The required config file to start odoo server. If not provided, we'll look for `.odoorc` file in the home directory."
        }
      }
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "odoo-dev-container",
          "title": "Odoo Dev",
          "icon": "resources/not-really-odoo-icon.svg"
        }
      ]
    },
    "views": {
      "odoo-dev-container": [
        {
          "id": "odoo-dev-branches",
          "name": "Branches"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/ini": "^1.3.31",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.77.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5"
  },
  "prettier": {
    "printWidth": 100
  },
  "dependencies": {
    "ini": "^4.1.0"
  }
}
