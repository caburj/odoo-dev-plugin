{
  "name": "odoo-dev-plugin",
  "displayName": "Odoo Dev",
  "description": "",
  "version": "0.0.1",
  "repository": {
    "url": "https://github.com/caburj/odoo-dev-plugin.git"
  },
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "icon": "./resources/not-really-odoo-icon.png",
  "extensionDependencies": [
    "vscode.git"
  ],
  "contributes": {
    "commands": [
      {
        "command": "odooDev.fetchBranch",
        "title": "Odoo Dev: Fetch",
        "icon": {
          "light": "resources/fetch-branch-light.svg",
          "dark": "resources/fetch-branch-dark.svg"
        }
      },
      {
        "command": "odooDev.createBranch",
        "title": "Odoo Dev: Create",
        "icon": {
          "light": "resources/plus-light.svg",
          "dark": "resources/plus-dark.svg"
        }
      },
      {
        "command": "odooDev.deleteBranch",
        "title": "Odoo Dev: Delete",
        "icon": {
          "light": "resources/delete-branch-light.svg",
          "dark": "resources/delete-branch-dark.svg"
        }
      },
      {
        "command": "odooDev.removeBranch",
        "title": "Odoo Dev: Remove",
        "icon": {
          "light": "resources/delete-branch-light.svg",
          "dark": "resources/delete-branch-dark.svg"
        }
      },
      {
        "command": "odooDev.checkoutBranch",
        "title": "Odoo Dev: Checkout",
        "icon": {
          "light": "resources/checkout-light.svg",
          "dark": "resources/checkout-dark.svg"
        }
      },
      {
        "command": "odooDev.selectBranch",
        "title": "Odoo Dev: Select",
        "icon": {
          "light": "resources/checkout-light.svg",
          "dark": "resources/checkout-dark.svg"
        }
      },
      {
        "command": "odooDev.fetchStableBranch",
        "title": "Odoo Dev: Fetch Stable"
      },
      {
        "command": "odooDev.resetActiveBranch",
        "title": "Odoo Dev: Reset Active Branch",
        "icon": {
          "light": "resources/refresh-light.svg",
          "dark": "resources/refresh-dark.svg"
        }
      },
      {
        "command": "odooDev.startFreshServer",
        "title": "Odoo Dev: Start Fresh Server",
        "icon": {
          "light": "resources/start-fresh-light.svg",
          "dark": "resources/start-fresh-dark.svg"
        }
      },
      {
        "command": "odooDev.stopActiveServer",
        "title": "Odoo Dev: Stop Active Server",
        "icon": {
          "light": "resources/stop-server-light.svg",
          "dark": "resources/stop-server-dark.svg"
        }
      },
      {
        "command": "odooDev.startServer",
        "title": "Odoo Dev: Start Server",
        "icon": {
          "light": "resources/start-server-light.svg",
          "dark": "resources/start-server-dark.svg"
        }
      },
      {
        "command": "odooDev.debugServer",
        "title": "Odoo Dev: Debug Server",
        "icon": {
          "light": "resources/debug-server-light.svg",
          "dark": "resources/debug-server-dark.svg"
        }
      },
      {
        "command": "odooDev.startServerWithInstall",
        "title": "Odoo Dev: Start Server With Install..."
      },
      {
        "command": "odooDev.debugServerWithInstall",
        "title": "Odoo Dev: Debug Server With Install..."
      },
      {
        "command": "odooDev.startServerWithUpdate",
        "title": "Odoo Dev: Start Server With Update..."
      },
      {
        "command": "odooDev.debugServerWithUpdate",
        "title": "Odoo Dev: Debug Server With Update..."
      },
      {
        "command": "odooDev.debugJS",
        "title": "Odoo Dev: Open Chrome (Debug)"
      },
      {
        "command": "odooDev.dropActiveDB",
        "title": "Odoo Dev: Drop Active DB",
        "icon": {
          "light": "resources/drop-db-light.svg",
          "dark": "resources/drop-db-dark.svg"
        }
      },
      {
        "command": "odooDev.getTestTag",
        "title": "Odoo Dev: Get Test Tag"
      },
      {
        "command": "odooDev.openChromeLocalServer",
        "title": "Odoo Dev: Open Chrome"
      },
      {
        "command": "odooDev.openOdooConf",
        "title": "Odoo Dev: Config File",
        "icon": {
          "light": "resources/config-file-light.svg",
          "dark": "resources/config-file-dark.svg"
        }
      },
      {
        "command": "odooDev.openLinkedNote",
        "title": "Odoo Dev: Linked Note",
        "icon": {
          "light": "resources/linked-note-light.svg",
          "dark": "resources/linked-note-dark.svg"
        }
      },
      {
        "command": "odooDev.openPullRequestLinkOdoo",
        "title": "Odoo Dev: Pull Request (odoo)"
      },
      {
        "command": "odooDev.openPullRequestLinkEnterprise",
        "title": "Odoo Dev: Pull Request (enterprise)"
      },
      {
        "command": "odooDev.openPullRequestLinkUpgrade",
        "title": "Odoo Dev: Pull Request (upgrade)"
      },
      {
        "command": "odooDev.openRunbotLink",
        "title": "Odoo Dev: Runbot"
      },
      {
        "command": "odooDev.isDependentOn",
        "title": "Odoo Dev: Is Addon Dependent On?"
      }
    ],
    "configuration": {
      "title": "Odoo Dev",
      "properties": {
        "odooDev.sourceFolder": {
          "type": "string",
          "description": "Folder where odoo/odoo repository is cloned. This folder is also expected to have odoo/enterprise and odoo/upgrade repositories."
        },
        "odooDev.remote.odoo": {
          "type": "string",
          "default": "",
          "description": "Remote to use to fetch branches in odoo repository."
        },
        "odooDev.remote.enterprise": {
          "type": "string",
          "default": "",
          "description": "Remote to use to fetch branches in enterprise repository."
        },
        "odooDev.remote.upgrade": {
          "type": "string",
          "default": "",
          "description": "Remote to use to fetch branches in upgrade repository."
        },
        "odooDev.branchNameAsDB": {
          "title": "Branch Name as DB",
          "type": "boolean",
          "default": false,
          "description": "Use the name of the branch as the name of the db."
        },
        "odooDev.pullBaseOnCreate": {
          "title": "Pull Base Branch on Create",
          "type": "boolean",
          "default": false,
          "description": "Activating this will try to pull from remote the base branch before creating (and checking out) the new branch."
        },
        "odooDev.odooConfigPath": {
          "title": "Config Path",
          "type": "string",
          "default": "",
          "description": "The required config file to start odoo server. If not provided, we'll look for `.odoorc` file in the home directory."
        },
        "odooDev.notesFolder": {
          "title": "Notes Folder",
          "type": "string",
          "default": "",
          "description": "This folder will contain the notes that will be linked to the branches."
        },
        "odooDev.autoStash": {
          "title": "Auto Stash",
          "type": "boolean",
          "default": false,
          "description": "Before switching (or creating) branches, stash the changes. When switching back, unstash the changes."
        },
        "odooDev.autoTest": {
          "title": "Auto Test",
          "type": "boolean",
          "default": false,
          "description": "If activated, when the cursor is in a test file, the server will be started in test mode."
        }
      }
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "odoo-dev-container",
          "title": "Odoo Dev",
          "icon": "resources/not-really-odoo-icon.svg"
        }
      ]
    },
    "views": {
      "odoo-dev-container": [
        {
          "id": "odoo-dev-branches",
          "name": "Branches"
        },
        {
          "id": "odoo-addons-tree",
          "name": "Dependency Tree"
        }
      ]
    },
    "submenus": [
      {
        "id": "odooDev.links",
        "label": "Odoo Dev Links",
        "icon": {
          "dark": "resources/links-dark.svg",
          "light": "resources/links-light.svg"
        }
      },
      {
        "id": "odooDev.openChrome",
        "label": "Odoo Dev: Open Chrome",
        "icon": {
          "light": "resources/chrome-light.svg",
          "dark": "resources/chrome-dark.svg"
        }
      }
    ],
    "menus": {
      "view/item/context": [
        {
          "submenu": "odooDev.links",
          "group": "inline@0",
          "when": "viewItem == dev-branch || viewItem == active-dev-branch"
        },
        {
          "command": "odooDev.selectBranch",
          "group": "inline@10",
          "when": "(viewItem == base-branch || viewItem == dev-branch) && (viewItem != active-base-branch && viewItem != active-dev-branch)"
        },
        {
          "command": "odooDev.openLinkedNote",
          "group": "inline@20",
          "when": "viewItem == base-branch || viewItem == dev-branch || viewItem == active-base-branch || viewItem == active-dev-branch"
        },
        {
          "command": "odooDev.resetActiveBranch",
          "group": "inline@30",
          "when": "viewItem == active-base-branch || viewItem == active-dev-branch"
        },
        {
          "command": "odooDev.deleteBranch",
          "group": "inline@40",
          "when": "viewItem == dev-branch || viewItem == active-dev-branch"
        },
        {
          "command": "odooDev.openRunbotLink",
          "group": "1_links@0",
          "when": "viewItem == dev-branch || viewItem == active-dev-branch"
        },
        {
          "command": "odooDev.openPullRequestLinkOdoo",
          "group": "2_github_links@10",
          "when": "viewItem == dev-branch || viewItem == active-dev-branch"
        },
        {
          "command": "odooDev.openPullRequestLinkEnterprise",
          "group": "2_github_links@20",
          "when": "viewItem == dev-branch || viewItem == active-dev-branch"
        },
        {
          "command": "odooDev.openPullRequestLinkUpgrade",
          "group": "2_github_links@30",
          "when": "viewItem == dev-branch || viewItem == active-dev-branch"
        },
        {
          "command": "odooDev.selectBranch",
          "group": "z_others@10",
          "when": "(viewItem == base-branch || viewItem == dev-branch) && (viewItem != active-base-branch && viewItem != active-dev-branch)"
        },
        {
          "command": "odooDev.openLinkedNote",
          "group": "z_others@20",
          "when": "viewItem == base-branch || viewItem == dev-branch || viewItem == active-base-branch || viewItem == active-dev-branch"
        },
        {
          "command": "odooDev.resetActiveBranch",
          "group": "z_others@30",
          "when": "viewItem == active-base-branch || viewItem == active-dev-branch"
        },
        {
          "command": "odooDev.deleteBranch",
          "group": "z_others@40",
          "when": "viewItem == dev-branch || viewItem == active-dev-branch"
        }
      ],
      "view/title": [
        {
          "command": "odooDev.createBranch",
          "when": "view == odoo-dev-branches",
          "group": "navigation@10"
        },
        {
          "command": "odooDev.fetchBranch",
          "when": "view == odoo-dev-branches",
          "group": "navigation@20"
        },
        {
          "command": "odooDev.openOdooConf",
          "when": "view == odoo-dev-branches",
          "group": "navigation@30"
        },
        {
          "command": "odooDev.dropActiveDB",
          "when": "view == odoo-dev-branches && !odooDev.hasActiveServer",
          "group": "navigation@40"
        },
        {
          "command": "odooDev.startServer",
          "when": "view == odoo-dev-branches && !odooDev.hasActiveServer",
          "group": "navigation@50"
        },
        {
          "command": "odooDev.debugServer",
          "when": "view == odoo-dev-branches && !odooDev.hasActiveServer",
          "group": "navigation@60"
        },
        {
          "command": "odooDev.stopActiveServer",
          "when": "view == odoo-dev-branches && odooDev.hasActiveServer",
          "group": "navigation@70"
        },
        {
          "submenu": "odooDev.openChrome",
          "when": "view == odoo-dev-branches && odooDev.hasActiveServer",
          "group": "navigation@80"
        },
        {
          "command": "odooDev.startFreshServer",
          "when": "view == odoo-dev-branches",
          "group": "1_start_fresh"
        },
        {
          "command": "odooDev.startServer",
          "when": "view == odoo-dev-branches",
          "group": "2_start@10"
        },
        {
          "command": "odooDev.startServerWithInstall",
          "when": "view == odoo-dev-branches",
          "group": "2_start@20"
        },
        {
          "command": "odooDev.startServerWithUpdate",
          "when": "view == odoo-dev-branches",
          "group": "2_start@30"
        },
        {
          "command": "odooDev.debugServer",
          "when": "view == odoo-dev-branches",
          "group": "3_debug@10"
        },
        {
          "command": "odooDev.debugServerWithInstall",
          "when": "view == odoo-dev-branches",
          "group": "3_debug@20"
        },
        {
          "command": "odooDev.debugServerWithUpdate",
          "when": "view == odoo-dev-branches",
          "group": "3_debug@30"
        }
      ],
      "odooDev.links": [
        {
          "command": "odooDev.openRunbotLink",
          "group": "1_links@0"
        },
        {
          "command": "odooDev.openPullRequestLinkOdoo",
          "group": "2_github_links@10"
        },
        {
          "command": "odooDev.openPullRequestLinkEnterprise",
          "group": "2_github_links@20"
        },
        {
          "command": "odooDev.openPullRequestLinkUpgrade",
          "group": "2_github_links@30"
        }
      ],
      "odooDev.openChrome": [
        {
          "command": "odooDev.openChromeLocalServer",
          "group": "1_chrome@0"
        },
        {
          "command": "odooDev.debugJS",
          "group": "1_chrome@10"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/ini": "^1.3.31",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/node-fetch": "^2.6.3",
    "@types/ps-tree": "^1.1.2",
    "@types/vscode": "^1.77.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "ini": "^4.1.0",
    "node-fetch": "^2.6.11",
    "ps-tree": "^1.2.0"
  },
  "prettier": {
    "printWidth": 100
  }
}
